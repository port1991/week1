var listWindow = Ti.UI.createWindow ({
	title: 'List Testing',
	backgroundImage: 'grid2.png',
	fullscreen: true
});
noteWindow.addEventListener('focus', function(){
	getNotes();
});

var wowNotes = Ti.UI.createTableView({
	layout : "vertical"
});

noteWindow.add(wowNotes);

exports.listWindow = listWindow;



