var win = Ti.UI.createWindow ({
	title: '',
	//barImage:'ironmanicon.png',
	backgroundImage: 'yellow',
	//fullscreen: true
	layout:'vertical'
});

var t1 = Ti.UI.createTextField({
	title: 'APP',
	width: 200,
	top: 20
});

var t2 = Ti.UI.createTextField({
	
	width: 200,
	top: 20
});

var t3 = Ti.UI.createTextField({
	
	width: 200,
	top: 20
});


win.add(t1);
win.add(t2);
win.add(t3);
win.open();

